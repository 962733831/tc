<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.java.dao.PrdMapper">

    <select id="getList" resultType="org.java.entity.Prd">
        select * from prd
    </select>

    <select id="findById" resultType="org.java.entity.Prd">
         select * from prd  where id = #{pid}
    </select>


</mapper>    